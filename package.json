{
  "name": "happiness-scss",
  "version": "5.1.0",
  "description": "SCSS happiness linter based on sass-lint",
  "main": "index.js",
  "bin": {
    "happiness-scss": "./bin/cmd.js"
  },
  "scripts": {
    "test": "happiness \"./*.js\" --verbose | snazzy && echo \"happiness-scss version\" && node ./bin/cmd.js -V && node ./bin/cmd.js -q -m 10",
    "happiness-fix": "happiness \"./*.js\" --fix | snazzy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dutchenkoOleg/happiness-scss.git"
  },
  "author": "Oleg Dutchenko <dutchenko.o.dev@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dutchenkoOleg/happiness-scss/issues"
  },
  "homepage": "https://github.com/dutchenkoOleg/happiness-scss#readme",
  "keywords": [
    "code-style",
    "happiness",
    "happiness-scss",
    "scss",
    "sass",
    "linter",
    "scss-lint",
    "sass-lint"
  ],
  "dependencies": {
    "sass-lint": "^1.11.1"
  },
  "devDependencies": {
    "happiness": "^10.0.2",
    "snazzy": "^7.0.0"
  }
}
